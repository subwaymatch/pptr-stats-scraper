generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Sport {
  id        String   @id
  name      String
  url       String
  createdAt DateTime @default(now())
}

model SchoolIndex {
  id        String   @id
  name      String
  iconUrl   String
  url       String
  createdAt DateTime @default(now())
}
