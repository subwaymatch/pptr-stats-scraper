generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Sport {
  id        String   @id
  name      String
  url       String
  createdAt DateTime @default(now())
}

model School {
  id          String   @id
  name        String
  conference  String?
  nickname    String?
  colors      String?
  bgColor     String?
  url         String?
  ncaaUrl     String
  twitterUrl  String?
  facebookUrl String?
  iconUrl     String
  createdAt   DateTime @default(now())
}
